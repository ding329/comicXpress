<nav class="navbar navbar-default navbar-fixed-top">
	<div class="container-fluid">
		<div class="navbar-header">
			<a class="navbar-brand" href="#">Order Attempt</a>
		</div>
		<div class="navbar-center search-field">
			{{#bs-form formLayout="inline" action="search"}}
				{{bs-input type="text" value=searchField placeholder="Search by item description"}}
				<button type="submit" class="btn btn-default">
					<span class="glyphicon glyphicon-search" aria-hidden="true"></span>
				</button>

	<!--			<label class="control-label">Search By Item Description:</label>
				{{bs-input type="text" value=searchField}}
	-->
			{{/bs-form}}
		</div>
	</div>
</nav>

<div class="container">
	<ul class= "list-group">
		{{#each catalog in catalogs}}
		<li class="list-group-item">
			<div class="media">
				<div class="media-left media-middle">
					{{catalog.itemId}}		{{catalog.name}}		{{catalog.orderDate}}		{{catalog.price}}	
				</div>
			</div>
		</li>
		{{/each}}
	</ul>
</div>
